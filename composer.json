{
  "name": "zxf/utils",
  "description": "现代化的 PHP 工具包，包含强大的 DOM 操作库、数据转换和一些常用的类库、HTTP网络请求、代码压缩、通用函数集和实用工具等",
  "type": "library",
  "keywords": [
    "utils",
    "util",
    "tools",
    "tool",
    "tree",
    "crypto",
    "file",
    "array",
    "curl",
    "dom",
    "html",
    "xml",
    "css",
    "xpath",
    "web-scraper",
    "html-parser",
    "xml-parser",
    "php",
    "php82",
    "php83",
    "php84",
    "工具",
    "DOM操作",
    "image",
    "http"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "zxf",
      "homepage": "https://yoc.cn"
    }
  ],
  "require": {
    "php": ">=8.2"
  },
  "require-dev": {
  },
  "autoload": {
    "psr-4": {
      "zxf\\Utils\\": "src/Utils/"
    },
    "files": [
      "src/funcs/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "zxf\\Utils\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "zxf\\Utils\\Providers\\UtilsServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "funding": [
    {
      "type": "custom",
      "url": "https://yoc.cn"
    }
  ],
  "support": {
    "issues": "https://github.com/zxf/utils/issues",
    "source": "https://github.com/zxf/utils"
  }
}